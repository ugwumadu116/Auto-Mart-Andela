{
  "name": "auto-mart-andela",
  "version": "1.0.0",
  "description": "Auto Mart is an online marketplace for automobiles of diverse makes, model or body type. With Auto Mart, users can sell their cars or buy from trusted dealerships or private sellers.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./build/index.js",
    "test": "nyc mocha -r esm --timeout 15000 --exit",
    "dev": "nodemon --exec babel-node ./server/index.js",
    "build": "npm run clean && npm run build-babel",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./server -s",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage"
  },
  "dependencies": {
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.14.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "esm": "^3.2.4",
    "express": "^4.17.0",
    "express-validator": "^5.3.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.0",
    "multer": "^1.4.1",
    "trim-request": "^1.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ugwumadu116/Auto-Mart-Andela.git"
  },
  "author": "joel ugwumadu",
  "license": "MIT",
  "engines": {
    "node": "10.13.0"
  },
  "bugs": {
    "url": "https://github.com/ugwumadu116/Auto-Mart-Andela/issues"
  },
  "homepage": "https://github.com/ugwumadu116/Auto-Mart-Andela#readme"
}